import java.lang.*;
import java.io.*;
class GFG
 {
	public static void main (String[] args)
	 {
	 Scanner sc = new Scanner(System.in);    
	 int t = sc.nextInt();
	 while(t>0)
	 {
	     int a = sc.nextInt();
	     
	     int result = a * ((2*a)+1);
	     
	     System.out.println(result);
	     t--;
	 }
	 }
}
