class GFG
 {
	public static void main (String[] args)
	 {
	    Scanner sc = new Scanner(System.in);
		int t = sc.nextInt();
		while(t>0)
		{
		    String str = sc.next();
		    int a = sc.nextInt();
		    int b = sc.nextInt();
		        System.out.println(str.substring(a,b+1));
		    t--;
		}
	 }
}
